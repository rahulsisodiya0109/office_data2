<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- Browsers support chrome, chrome-headless, chrome-ng, chrome-android -->
<suite name="RWS Automation Suite" verbose="1">
	<parameter name="browser" value="chrome-android" />
	<test name="Regression">
		<classes>
			<class name="suite.base.TestBase" />
			<class name="suite.init.LivemintMobileTest" />
		</classes>
	</test>
</suite>

